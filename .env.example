# Copy to .env and fill in values before using docker-compose
# Example .env file for zako-issuetracker

# Discord bot token (required)
DISCORD_TOKEN=your_discord_bot_token_here

# Comma-separated admin Discord user IDs (recommended)
ADMIN_IDS=123456789012345678,987654321098765432

# SQLite file path inside the container (optional; default is /app/data/zako.db)
SQLITE_FILE=/app/data/zako.db
